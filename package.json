{
  "name": "taro-react-quick-start",
  "version": "1.0.0",
  "private": false,
  "description": "Quick start project with Taro & React & TypeScript.",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
